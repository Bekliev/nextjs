{
  "version": 2,
  "name": "nextjs",
  "alias": "nextjs.now.sh",
  "builds": [
    { "src": "/static/*", "use": "@now/static" },
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/blog/(?<slug>.+)", "dest": "/blog/post?title=$slug" },
    { "src": "/tv-shows/(?<slug>.+)", "dest": "/tv-shows/post?id=$slug" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
